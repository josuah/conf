#!/usr/bin/awk -f

BEGIN {
	cmd = "dd if=/dev/urandom bs=8 count=1 2>/dev/null | od -An -td8 "
	cmd | getline
	close(cmd)
	srand($0)
}

{
	LINE[NR] = $0
}

END {
	while (length(LINE) > 0) {
		i = int(rand() * length(LINE))
		for (l in LINE) {
			if (i-- == 0) {
				print(LINE[l])
				delete LINE[l]
			}
		}
	}
}
